    "babel-preset-react-hmre": "^1.1.1",
    "bootstrap": "^3.3.6",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "eslint-watch": "^2.1.11",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "immutable": "^3.8.1",
    "jquery": "^2.2.4",

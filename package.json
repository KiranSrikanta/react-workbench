{
  "name": "workbench",
  "version": "1.0.0",
  "description": "Place to try out react stuff",
  "main": "src/server/app.js",
  "scripts": {
    "start": "npm-run-all --parallel build:watch test:watch lint:watch",
    "build": "webpack -p",
    "build:watch": "webpack-dev-server --content-base ./src/client/public --port 8000 --inline --hot --info false",
    "build:watch:info": "webpack-dev-server --content-base ./src/client/public --port 8000 --inline --hot",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "lint": "eslint src/client/app/**/*.js",
    "lint:watch": "esw src/client/app/**/*.js -w"
  },
  "author": "Kiran",
  "license": "ISC",

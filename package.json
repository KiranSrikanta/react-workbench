{
  "name": "workbench",
  "version": "1.0.0",
  "description": "Place to try out react stuff",
  "main": "src/server/app.js",
  "scripts": {
    "start": "npm-run-all --parallel dev test:watch lint:watch",
    "start:prod": "npm-run-all build:prod prod",
    "build:prod": "set NODE_ENV=production&&webpack -p",
    "dev": "set NODE_ENV=development&&node src/server/app.js",
    "prod": "set NODE_ENV=production&&node src/server/app.js",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "lint": "eslint src/client/app/**/*.js",
    "lint:watch": "esw src/client/app/**/*.js -w",
    "coverage": "karma start karma.coverage.conf.js"
  },
  "author": "Kiran",
  "license": "ISC",
  "devDependencies": {
    "babel-loader": "^6.2.4",
